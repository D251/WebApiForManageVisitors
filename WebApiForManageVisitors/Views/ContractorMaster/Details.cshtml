@model WebApiForWorkPermitSystem.Models.ContractorMasterModel

@{
    ViewBag.Title = "Details";
}

<div class="content-header clearfix">
    <h1 class="pull-left">
        Contractor Master Details
        <small><i class="fa fa-arrow-circle-left"></i>@Html.ActionLink("  Back to List", "Index")</small>
    </h1>
    <div class="pull-right">
        <a href="@Url.Action("Edit", new { id = Model.ContractorSrNo })" class="btn bg-blue">
            <i class="fa fa-plus-square"></i>
            Edit
        </a>
    </div>
</div>
<div class="content-header clearfix">
    <div class="box box-info">
        <div class="box-body">
            <div class="form-horizontal">
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                <div class="form-group">
                    @Html.LabelFor(model => model.ContractorSrNo, htmlAttributes: new { @class = "control-label col-md-2" })
                    <div class="col-md-5">
                        @Html.EditorFor(model => model.ContractorSrNo, new { htmlAttributes = new { @class = "form-control", @readonly = true, @style = "background-color:white" } })
                        @Html.ValidationMessageFor(model => model.ContractorSrNo, "", new { @class = "text-danger" })
                    </div>
                </div>


                <div class="form-group">
                    @Html.LabelFor(model => model.CompanyName, htmlAttributes: new { @class = "control-label col-md-2" })
                    <div class="col-md-5">
                        @Html.EditorFor(model => model.CompanyName, new { htmlAttributes = new { @class = "form-control", @readonly = true, @style = "background-color:white" } })
                        @Html.ValidationMessageFor(model => model.CompanyName, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group">
                    @Html.LabelFor(model => model.ContractorName, htmlAttributes: new { @class = "control-label col-md-2" })
                    <div class="col-md-5">
                        @Html.EditorFor(model => model.ContractorName, new { htmlAttributes = new { @class = "form-control", @readonly = true, @style = "background-color:white" } })
                        @Html.ValidationMessageFor(model => model.ContractorName, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group">
                    @Html.LabelFor(model => model.ContractorContactNo, htmlAttributes: new { @class = "control-label col-md-2" })
                    <div class="col-md-5">
                        @Html.EditorFor(model => model.ContractorContactNo, new { htmlAttributes = new { @class = "form-control", @readonly = true, @style = "background-color:white" } })
                        @Html.ValidationMessageFor(model => model.ContractorContactNo, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group">
                    @Html.LabelFor(model => model.ContractorCreateDate, htmlAttributes: new { @class = "control-label col-md-2" })
                    <div class="col-md-5">
                        @Html.EditorFor(model => model.ContractorCreateDate, new { htmlAttributes = new { @class = "form-control", @readonly = true, @style = "background-color:white" } })
                        @Html.ValidationMessageFor(model => model.ContractorCreateDate, "", new { @class = "text-danger" })
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
}

