@model IEnumerable<WebApiForManageVisitors.Models.tbl_VisitorUserRegistration>

@{
    ViewBag.Title = "Index";
}
<section class="content-header">
    <h1 class="pull-left">
        Visitor Registration Details
    </h1>
    <div class="pull-right">
        <a href="@Url.Action("Create")" class="btn bg-blue">
            <i class="fa fa-plus-square"></i>
            Create New
        </a>
    </div>
</section>
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <!-- /.box-header -->
                <div class="box-body">
                    <table id="example1" class="display" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.VisitorUserID)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.VisitorName)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.VisitorAddress)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.VisitorContactNo)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.VisitorEmailID)
                                </th>
                                @*<th>
                                        @Html.DisplayNameFor(model => model.VisitorNatureOfWork)
                                    </th>*@
                                <th>
                                    @Html.DisplayNameFor(model => model.VisitorContractor)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.VisitorContractorCoNo)
                                </th>
                                @*<th>
                                        @Html.DisplayNameFor(model => model.VisitorPassword)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.VisitorRegistrationDate)
                                    </th>*@
                                <th></th>
                            </tr>
                        </thead>
                        <tfoot>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.VisitorUserID)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.VisitorName)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.VisitorAddress)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.VisitorContactNo)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.VisitorEmailID)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.VisitorContractor)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.VisitorContractorCoNo)
                                </th>
                            </tr>
                        </tfoot>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.VisitorUserID)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.VisitorName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.VisitorAddress)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.VisitorContactNo)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.VisitorEmailID)
                                    </td>
                                    @*<td>
                                            @Html.DisplayFor(modelItem => item.VisitorNatureOfWork)
                                        </td>*@
                                    <td>
                                        @Html.DisplayFor(modelItem => item.VisitorContractor)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.VisitorContractorCoNo)
                                    </td>
                                    @*<td>
                                            @Html.DisplayFor(modelItem => item.VisitorPassword)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.VisitorRegistrationDate)
                                        </td>*@
                                    <td>
                                        @Html.ActionLink("Edit", "Edit", new { id = item.VisitorSrNo }) |
                                        @Html.ActionLink("Details", "Details", new { id = item.VisitorSrNo }) |
                                        @Html.ActionLink("Delete", "Delete", new { id = item.VisitorSrNo })
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>

