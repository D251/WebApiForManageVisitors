@model WebApiForManageVisitors.Models.tbl_DesignationMasterModel

    @{
        ViewBag.Title = "Details";
    }

    <div class="content-header clearfix">
        <h1 class="pull-left">
            Designation Master Details
            <small><i class="fa fa-arrow-circle-left"></i>@Html.ActionLink("  Back to List", "Index")</small>
        </h1>
        <div class="pull-right">
            <a href="@Url.Action("Edit", new { id = Model.DesignationID })" class="btn bg-blue">
                <i class="fa fa-plus-square"></i>
                Edit
            </a>
        </div>
    </div>
  <div class="content-header clearfix">
    <div class="box box-info">
        <div class="box-body">
            <div class="form-horizontal">
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                <div class="form-group">
                    @Html.LabelFor(model => model.DesignationID, htmlAttributes: new { @class = "control-label col-md-2" })
                    <div class="col-md-5">
                        @Html.EditorFor(model => model.DesignationID, new { htmlAttributes = new { @class = "form-control", @readonly = true, @style = "background-color:white" } })
                        @Html.ValidationMessageFor(model => model.DesignationID, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group">
                    @Html.LabelFor(model => model.DepartmentName, htmlAttributes: new { @class = "control-label col-md-2" })
                    <div class="col-md-5">
                        @Html.EditorFor(model => model.DepartmentName, new { htmlAttributes = new { @class = "form-control", @readonly = true, @style = "background-color:white" } })
                        @Html.ValidationMessageFor(model => model.DepartmentName, "", new { @class = "text-danger" })
                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(model => model.DesignationName, htmlAttributes: new { @class = "control-label col-md-2" })
                    <div class="col-md-5">
                        @Html.EditorFor(model => model.DesignationName, new { htmlAttributes = new { @class = "form-control", @readonly = true, @style = "background-color:white" } })
                        @Html.ValidationMessageFor(model => model.DesignationName, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group">
                    @Html.LabelFor(model => model.DesignationCreateDate, htmlAttributes: new { @class = "control-label col-md-2" })
                    <div class="col-md-5">
                        @Html.EditorFor(model => model.DesignationCreateDate, new { htmlAttributes = new { @class = "form-control", @readonly = true, @style = "background-color:white" } })
                        @Html.ValidationMessageFor(model => model.DesignationCreateDate, "", new { @class = "text-danger" })
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
}

